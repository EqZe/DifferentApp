
{
  "expo": {
    "name": "Different",
    "slug": "differentapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/different logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/different logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilayrachkovski.different",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/different logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.differentapp.co.il",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "onesignal-expo-plugin",
        {
          "mode": "production",
          "appId": "b732b467-6886-4c7b-b3d9-5010de1199d6",
          "appIdentifier": "com.ilayrachkovski.different",
          "androidPackage": "com.differentapp.co.il"
        }
      ],
      "./plugins/fix-material-theme.js"
    ],
    "scheme": "different",
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": false
    },
    "extra": {
      "router": {},
      "backendUrl": "https://8fazvn3hy8nmzjsg6kz2u68p6xpbp6gk.app.specular.dev",
      "eas": {
        "projectId": "fe404aca-e46f-42c2-ac3a-50c265d87ae7"
      }
    }
  }
}
